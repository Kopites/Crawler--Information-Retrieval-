   i   z   e       /       2   )   ;       i   f       (   i   w       %       2   )       i   w   -   -   ;       i   h       =       i   w       +       p   p   1   ;       i   l       =       -   (   p   a   r   s   e   I   n   t   (   (   i   w       +       2   )       /       2   )   )   ;       i   f       (   h   e   a   d       &&    g   a   p   )       i   h       +   =       a   g   ;       e   l   s   e       i   h       +   =       a   f   ;       i   w       +   =       a   f   ;       i   f       (   d       =   =       "   d   o   w   n   "   )       {       i   f       (   g   a   p   )       p   p   +   +   ;       i   t       =       -   i   h       -       p   p       +       a   g   ;       b   b       =       0   ;       }       e   l   s   e       {       i   t       =       s   i   z   e       -       1       +       p   p       +       a   g   ;       b   t       =       0   ;       }       }       e   l   s   e       {       i   h       =       p   a   r   s   e   I   n   t   (   s   i   z   e       /       2   )   ;       i   f       (   i   h       %       2   )       i   h   -   -   ;       i   w       =       i   h       +       p   p   1   ;       i   t       =       -   (   p   a   r   s   e   I   n   t   (   (   i   w       +       2   )       /       2   )   )   ;       i   f       (   h   e   a   d       &&    g   a   p   )       i   w       +   =       a   g   ;       e   l   s   e       i   w       +   =       a   f   ;       i   h       +   =       a   f   ;       i   f       (   d       =   =       "   r   i   g   h   t   "   )       {       i   l       =       -   i   h       -       1       -       p   p       +       a   g   ;       b   r       =       0   ;       }       e   l   s   e       {       i   l       =       s   i   z   e       -       1       +       p   p       +       a   g   ;       b   l       =       0   ;       }       }       i   f       (   g   a   p   )       {       b   t       =       1   ;       b   r