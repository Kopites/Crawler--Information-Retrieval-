function submitSubscribe()
{
	var email =  document.getElementById("sub_email").value;
	var sub_captcha = "";
	var section = "news";
	var article_url = "";
	var subreq;
	if(section == "business"){section = "money" ;}
	
	if(!validEmail(email))
	{
		alert("Please enter a valid email");
		document.getElementById("sub_email").focus();
		return false;
	}
	sub_fillTacking('email','sub_email'); 
	/*else if(sub_captcha == "" || sub_captcha == "Enter here the characters shown below")
	{
		alert("Please enter captcha code");
		document.getElementById("sub_captcha").focus();
		return false;
	}*/


	var url= cdn_domain+"/newsletter";
	var postdata = "email="+email+"&captcha="+sub_captcha+"&section="+section+"&article_url="+encodeURIComponent(article_url)+"&country=non-india&output=xml";
	if (window.XMLHttpRequest)
	{
		subreq = new XMLHttpRequest();
		subreq.onreadystatechange = statusstateChanged;
		subreq.open("POST", url, true);
		subreq.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
		subreq.setRequestHeader("Content-Length", postdata.length.toString());
		subreq.setRequestHeader("Connection", "Close");
		subreq.send(postdata);
	}
	else if (window.ActiveXObject)
	{
		subreq = new ActiveXObject("Microsoft.XMLHTTP");
		if (subreq)
		{
			subreq.onreadystatechange = statusstateChanged;
			subreq.open("POST", url, true);
			subreq.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
			subreq.setRequestHeader("Content-Length", postdata.length);
			subreq.setRequestHeader("Connection", "Close");
			subreq.send(postdata);
		}
	}

	function statusstateChanged()
	{
		if (subreq.readyState==4 || subreq.readyState=="complete")
		{
			if(subreq.status == 200)
			{
				var resp_status = (subreq.responseXML.getElementsByTagName("status")[0].firstChild)?subreq.responseXML.getElementsByTagName("status")[0].firstChild.nodeValue:0;
				var resp_message = (subreq.responseXML.getElementsByTagName("message")[0].firstChild)?subreq.responseXML.getElementsByTagName("message")[0].firstChild.nodeValue:0;
				if(resp_status == "Success")
				{
					document.getElementById("div_subscribe2").innerHTML = "<div style='height:50px;overflow:hidden;border:solid 1px #ccc;background:#fff;text-align:center;'><div title='Close' style='float:right;color:#0066bc;font:bold 16px arial;' class='floatR bold curhand f14' onclick='closeSubscripBox();'>x &nbsp;</div><span class='clear'></span><p class='f16 bold alignC' style='font-size:16px;'>Thank you for signing up for the Rediff NewsLetter!</p></div>" ;
					if(document.getElementById('metric_NLiframe')){document.getElementById('metric_NLiframe').src = "http://metric.ind.rediff.com/www.rediff.com/nlsubscribe/thanks?rkey="+Math.random();}
				}
				else
				{
					document.getElementById("div_subscrip_message").innerHTML  = "<p class=red>"+resp_message+"</p>"; 
					document.getElementById('div_subscrip_message').style.display = "block";
					//refresgCapcha();
					if(document.getElementById("subscribe_btn")){document.getElementById("subscribe_btn").disabled = false;}
				}
			}
		}
		if (subreq.readyState == 1)
		{
			if(document.getElementById("subscribe_btn")){document.getElementById("subscribe_btn").disabled = true;}
			//wait
		}
	}
	return false;
}

